{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "0.9.3",
    "sw-precache": "^5.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0",
    "react-toolbox": "2.0.0-beta.7",
    "react-toolbox-themr": "^1.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && sw-precache --config=sw-precache-config.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "targeted-build-helper": "sh -ac '. ./.env.${REACT_APP_BUILD_TARGET}; npm run build'",
    "build:deploy": "REACT_APP_BUILD_TARGET=deploy npm run targeted-build-helper",
    "toolbox": "react-toolbox-themr"
  },
  "reactToolbox": {
    "include": [
      "APP_BAR",
      "DRAWER",
      "LAYOUT",
      "LIST"
    ],
    "output": "assets/react-toolbox"
  }
}
